{
  "presets": [
    "react",
    "es2015",
    "stage-3"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "dynamic-import-node",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-function-bind",
    "ramda"
  ],

  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
